{
"name": "TrackSOM",
"description": "TrackSOM clustering method.",
"tags": ["flow cytometry", "clustering"],
"authors": ["tercen"],
"urls": ["https://github.com/tercen/tracksom_operator"],
"container": "tercen/tracksom_operator:0.1.5",
  "properties": [
  {
    "kind": "BooleanProperty",
    "name": "tracking",
    "defaultValue": true,
    "description": "Whether to track cluster changes (TRUE), or just cluster (FALSE)."
  },
  {
    "kind": "BooleanProperty",
    "name": "noMerge",
    "defaultValue": false,
    "description": "Whether to allow meta-cluster merging (FALSE) or not (TRUE)."
  },
  {
    "kind": "StringProperty",
    "name": "nclust",
    "defaultValue": "NULL",
    "description": "Number of clusters to infer"
  },
  {
    "kind": "StringProperty",
    "name": "maxMeta",
    "defaultValue": "NULL",
    "description": "Maximal number of clusters (if specified, nclust is ignored). If both nclust and maxMeta are NULL, maxMeta is set to 10."
  },
  {
    "kind": "DoubleProperty",
    "name": "seed",
    "defaultValue": -1,
    "description": "Seed for random number generation. if less than 0, set a Random seed"
  },
  {
    "kind": "DoubleProperty",
    "name": "xdim",
    "defaultValue": 10,
    "description": "Size of x dim for the SOM grid"
  },
  {
    "kind": "DoubleProperty",
    "name": "ydim",
    "defaultValue": 10,
    "description": "Size of y dim for the SOM grid"
  },
  {
    "kind": "DoubleProperty",
    "name": "rlen",
    "defaultValue": 10,
    "description": "Size of rlen"
  },
   {
    "kind": "DoubleProperty",
    "name": "mst",
    "defaultValue": 1,
    "description": "Amount of training"
  },
  {
    "kind": "DoubleProperty",
    "name": "alpha_1",
    "defaultValue": 0.05,
    "description": "Alpha1 setting"
  },
   {
    "kind": "DoubleProperty",
    "name": "alpha_2",
    "defaultValue": 0.01,
    "description": "Alpha2 setting"
  },
  {
    "kind": "DoubleProperty",
    "name": "distf",
    "defaultValue": 2,
    "description": "Distance setting"
  }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
          "kind": "CrosstabSpec",
          "metaFactors": [
              {
                  "kind": "MetaFactor",
                  "name": "Timepoint",
                  "type": "",
                  "description": "Represents the timepoint (e.g. measurement day)",
                  "ontologyMapping": "timepoint",
                  "crosstabMapping": "column",
                  "cardinality": "1..n",
                  "factors": []
              },
              {
                  "kind": "MetaFactor",
                  "name": "Variables",
                  "type": "",
                  "description": "Represents the variables (e.g. channels, markers)",
                  "ontologyMapping": "variable",
                  "crosstabMapping": "column",
                  "cardinality": "1..n",
                  "factors": []
              },
              {
                  "kind": "MetaFactor",
                  "name": "Clusters",
                  "type": "",
                  "description": "Represents the clusters (e.g. cells)",
                  "ontologyMapping": "observation",
                  "crosstabMapping": "row",
                  "cardinality": "1..n",
                  "factors": []
              }
          ],
          "axis": [
              {
                  "kind": "AxisSpec",
                  "metaFactors": [
                      {
                          "kind": "MetaFactor",
                          "name": "Measurement",
                          "type": "numeric",
                          "description": "Value of measurement signal of the channel/marker",
                          "ontologyMapping": "measurement",
                          "crosstabMapping": "y",
                          "cardinality": "1",
                          "factors": []
                      }
                  ]
              }
          ]
      }
  ]
  }
}