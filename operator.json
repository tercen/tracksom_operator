{
"name": "TrackSOM operator",
"description": "TrackSOM clustering method.",
"tags": ["flow cytometry", "clustering"],
"authors": ["tercen"],
"urls": ["https://github.com/tercen/tracksom_operator"],
"container":"tercen/tracksom_operator:0.0.3",
  "properties": [
   {
    "kind": "StringProperty",
    "name": "nclust",
    "defaultValue": "NULL",
    "description": "Number of clusters to infer"
  },
  {
    "kind": "StringProperty",
    "name": "maxMeta",
    "defaultValue": "NULL",
    "description": "Maximal number of clusters (if specified, nclust is ignored). If both nclust and masMeta are NULL, maxMeta is set to 10."
  },
  {
    "kind": "DoubleProperty",
    "name": "seed",
    "defaultValue": -1,
    "description": "Seed for random number generation. if less than 0, set a Random seed"
  },
  {
    "kind": "DoubleProperty",
    "name": "xdim",
    "defaultValue": 10,
    "description": "Size of x dim for the SOM grid"
  },
  {
    "kind": "DoubleProperty",
    "name": "ydim",
    "defaultValue": 10,
    "description": "Size of y dim for the SOM grid"
  },
  {
    "kind": "DoubleProperty",
    "name": "rlen",
    "defaultValue": 10,
    "description": "Size of rlen"
  },
   {
    "kind": "DoubleProperty",
    "name": "mst",
    "defaultValue": 1,
    "description": "Amount of training"
  },
   {
    "kind": "DoubleProperty",
    "name": "alpha_1",
    "defaultValue": 0.05,
    "description": "Alpha1 setting"
  },
   {
    "kind": "DoubleProperty",
    "name": "alpha_2",
    "defaultValue": 0.01,
    "description": "Alpha2 setting"
  },
  {
    "kind": "DoubleProperty",
    "name": "distf",
    "defaultValue": 2,
    "description": "Distance setting"
  }
  ]
}
